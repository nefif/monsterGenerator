package model.bean;

/**
 *
 * @author nefif
 */
public class Jogador {

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNomeDoJogador() {
        return nomeDoJogador;
    }

    public void setNomeDoJogador(String nomeDoJogador) {
        this.nomeDoJogador = nomeDoJogador;
    }

    public String getClasseDoPersonagem() {
        return classeDoPersonagem;
    }

    public void setClasseDoPersonagem(String classeDoPersonagem) {
        this.classeDoPersonagem = classeDoPersonagem;
    }

    public int getNivelDoPersonagem() {
        return nivelDoPersonagem;
    }

    public void setNivelDoPersonagem(int nivelDoPersonagem) {
        this.nivelDoPersonagem = nivelDoPersonagem;
    }
    int id;
    String nomeDoJogador;
    String classeDoPersonagem;
    int nivelDoPersonagem;
}


